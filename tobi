let people = ["Greg", "Mary", "Berry", "James"];

        for(i = 0; i < people.length; i++){
            console.log(people[i])
        }
        people.shift()
        console.log(people)
        people.pop()
        console.log(people)
        people.unshift("David")
        console.log(people)
        people.push("Ayo")
        console.log(people)
        
        for(i = 0; i < 2; i++){
            console.log(people[i])
        }
        let copy = people.slice(0,2)
        console.log(copy)

        let programming = {
            languages: ["JavaScript","Python","Ruby"],
            isChallenging: true,
            isRewareding: true,
            difficulty: 8,
            jokes: "https://www.devtopics.com/best-programming-jokes/"
        }
        programming.languages.push("Go")
       console.log(programming.languages)
       programming.difficulty = 7
       console.log(programming.difficulty)
